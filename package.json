  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "firebase": "^6.1.0",
    "firebase-tools": "^6.10.0",
    "husky": "^2.3.0",
    "leaflet": "^1.5.1",
    "leaflet-control-geocoder": "^1.7.0",
    "lint-staged": "^8.1.7",
